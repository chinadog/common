}

QRadioButton::indicator:unchecked:pressed
{
  border:1px solid #5F7E8D;
}

QRadioButton::indicator::checked
{
  image: url("$$PREFIX/icons/dark/radio_checked.png");
  border:1px solid #248ec6;
}

QRadioButton::indicator:checked:hover
{
}

QRadioButton::indicator:checked:pressed
{
}

/* --- QSCROLLBAR --- */

QScrollBar:vertical
{
  background:rgba(0,0,0,0);
  width:10px;
  margin: 0px 0px 0px 0px;
  border-radius:0px;
}
QScrollBar::handle:vertical
{
  background: #878787;
  min-height: 50px;
  border-radius:3px;
  width:10px;
  margin:2px;
}
QScrollBar::add-line:vertical
{
  height: 0px;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
  border-radius:5px;
}
QScrollBar::sub-line:vertical
{
  height: 0px;
  subcontrol-position: top;
  subcontrol-origin: margin;
  border-radius:5px;
}
QScrollBar::sub-page:vertical
{
}
QScrollBar::add-page:vertical
{
}

/*---------------------------------*/

QScrollBar:horizontal
{
  background:#232323;
  height:10px;
  margin: 0px 0px 0px 0px;
  border-radius:0px;
}
QScrollBar::handle:horizontal
{
  background: #878787;
  min-width: 50px;
  border-radius:3px;
  height:10px;
  margin:2px;
}
QScrollBar::add-line:horizontal
{
  width: 0px;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
  border-radius:5px;
}
QScrollBar::sub-line:horizontal
{
  width: 0px;
  subcontrol-position: top;
  subcontrol-origin: margin;
  border-radius:5px;
}
QScrollBar::sub-page:horizontal
{
}
QScrollBar::add-page:horizontal
{
}

/* ---------- QSPINBOX ------------*/

QSpinBox,QDoubleSpinBox
{
  border:none;
  outline:none;
  border-radius:3px;
  padding:5px;
  background-color:#232323;
  border:1px solid #4a4a4a;
  color:#ececec;
}
QSpinBox:hover,QDoubleSpinBox:hover
{
  border: 1px solid #406fa0;
}
QSpinBox::up-button, QDoubleSpinBox::up-button
{
  subcontrol-origin: border;
  subcontrol-position: top right;
  background-image:url("$$PREFIX/icons/dark/up6.png");
  background-repeat:no-repeat;
  background-position: center;
  background-color:rgba(0,0,0,0);
}
QSpinBox::down-button, QDoubleSpinBox::down-button
{
  subcontrol-origin: border;
  subcontrol-position: bottom right;
  background-image:url("$$PREFIX/icons/dark/down6.png");
  background-repeat:no-repeat;
  background-position: center;
  background-color:rgba(0,0,0,0);
}

/* --- QSPLITTER --- */

QSplitter::handle
{
  image: url(images/batman.png);
  background-color:#000;
}

/* --- QSTATUSBAR ---*/

QStatusBar
{
  background-color:rgba(0,0,0,0);
}
QStatusBar::item
{
  border-right:1px solid #636363;
}
QStatusBar QLabel
{
  padding-top:0px;
  padding-bottom:0px;
}

/* --- QTAB --- */

QTabWidget::pane
{
  top:-2px;
  border:2px solid #384672;
  border-right:1px solid #232323;
  border-bottom:1px solid #232323;
  border-radius: 3px;
  padding:2px;
}
QTabWidget::pane[type="minimalize"]
{
  border-radius:0px;
  border-right: 0px;
  border-left:0px;
  border-bottom:0px;
}
QTabWidget::tab-bar
{
  left: 10px;
}
QTabBar::tab
{
  min-width: 8ex;
  padding: 3px;
  padding-left:10px;
  padding-right:10px;
  color:#9a9a9a;
  outline:none;
}
QTabBar::close-button
{
  image: url("$$PREFIX/icons/dark/cross2.png");
  subcontrol-position: right;
  subcontrol-origin: padding;
  height: 5px;
  width: 5px;
  max-height:5px;
  max-width:5px;
}
QTabBar::close-button:hover
{
  image: url("$$PREFIX/icons/multicolor/cross.png");
  subcontrol-position: right;
}
QTabBar::tab:hover
{
  color:#ececec;
}
QTabBar::tab:selected
{
  border-bottom: 1px solid #384672;
  background: #384672;
  color:#ececec;
}
QTabBar::tab:!selected
{
  border-bottom:2px solid #384672;
  background-color:#253050;
}
QTabBar::tear
{
  opacity: 120;
  border: none;
  background-color:qlineargradient(x1:0, y1:0, x2:1, y2:0,
                    stop:0 rgba(0,0,0,255),
                    stop:1 rgba(0,0,0,0));
  border-bottom: 1px solid #636363;
}
QTabBar::scroller
{
  width: 20px;
  margin: 1px;
}
QTabBar QToolButton /* кнопки прокрутки - инструментальные кнопки */
{
  border:0px;
  border-radius:0px;
  border-bottom:1px solid #636363;
  background-color:#353535;
}
QTabBar QToolButton::right-arrow
{
  image: url("$$PREFIX/icons/dark/right.svg");
}
QTabBar QToolButton::left-arrow
{
  image: url("$$PREFIX/icons/dark/left.svg");
}

QTabBar:tab:selected{margin-top:0px;}
QTabBar:tab:!selected
{
  border-left:0px;
  margin-top:3px;
  border-top-left-radius: 0px;
  border-top-right-radius: 0px;
}
QTabBar:tab:last { border-top-right-radius: 0px;}
QTabBar:tab:first { border-top-left-radius: 0px; border-left: 0px solid #5a5a5a;}
QTabBar:tab:only-one { 	margin-top: 3px;}

